<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <ListView
        android:id="@+id/numberListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/list_view_horizontal_margin"
        android:layout_marginEnd="@dimen/list_view_horizontal_margin"
        android:layout_marginTop="@dimen/list_view_vertical_margin"
        android:layout_marginBottom="@dimen/list_view_vertical_margin"/>
</FrameLayout>